{
  "name": "urkit",
  "version": "0.1.5",
  "description": "A modern, accessible UI component library for Nuxt 4 with custom CSS architecture",
  "author": "Jaber Elferk <elferkjaber@gmail.com>",
  "license": "MIT",
  "type": "module",
  "keywords": [
    "nuxt",
    "nuxt4",
    "nuxt-module",
    "ui",
    "components",
    "vue",
    "css",
    "button",
    "input",
    "form",
    "urkit",
    "ui-library",
    "component-library"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jaeel29/urkit.git"
  },
  "bugs": {
    "url": "https://github.com/jaeel29/urkit/issues"
  },
  "homepage": "https://github.com/jaeel29/urkit#readme",
  "exports": {
    ".": {
      "types": "./dist/types.d.ts",
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist",
    "src/runtime/public"
  ],
  "scripts": {
    "prepack": "nuxt-module-build build",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxi prepare playground",
    "release": "npm run lint && npm run prepack && npm publish && git push --follow-tags",
    "lint": "eslint .",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:types": "vue-tsc --noEmit"
  },
  "dependencies": {
    "@nuxt/kit": "^4.0.0"
  },
  "devDependencies": {
    "@nuxt/devtools": "latest",
    "@nuxt/module-builder": "latest",
    "@nuxt/schema": "^4.0.0",
    "eslint": "^9.0.0",
    "nuxt": "^4.0.0",
    "typescript": "latest",
    "unbuild": "latest",
    "vitest": "^2.0.0",
    "vue": "latest",
    "vue-tsc": "^2.0.0"
  }
}
